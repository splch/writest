{
    "name": "writest",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Bring you from a writer to the writest.",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "scripts": ["worker.js"],
    "permissions": [
        "tabs",
        "<all_urls>"
    ]
}